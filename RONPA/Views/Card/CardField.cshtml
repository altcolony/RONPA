@model RONPA.ViewModels.CardFieldVM;


<div class="">
    <button onclick="handleRegisterCard()" class="btn btn-primary">新規登録</button>
</div>
<div id="addCardSpace" class="row">
    @{
        for (int i = 0; i < Model.CardVMs.Count; i++)
        {
            <partial name="_Card" model="Model.CardVMs[i]" />
        }
    }
</div>

@section Scripts{
    <script src="~/js/card.js"></script>
}